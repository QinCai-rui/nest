generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Domain {
    domain    String   @id @unique
    createdAt DateTime @default(now())
    username  String
    proxy     String
}

model users {
  tilde_username String    @unique @db.VarChar(255)
  email          String?   @db.VarChar(255)
}